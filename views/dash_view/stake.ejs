      <div class="col-12">
        <div class="card custom-card">
          <div class="card-body">
            <h4 class="main-content-label mb-3 text-white">Stake History</h4>
            <div class="table-responsive">
              <table
                class="table table-borderless table-striped table-hover text-nowrap text-md-nowrap table-hover mg-b-0"
              >
                <thead>
                  <tr>
                    <th><center>ID</center></th>
                    <th><center>Token</center></th>
                    <th><center>Amount</center></th>
                    <th><center>Est. APY</center></th>
                    <th><center>Duration</center></th>
                    <th><center>Date</center></th>
                  </tr>
                </thead>

                <tbody></tbody>
              </table>
             
            </div>
             <script>
    async function loadStakes() {
        try {
            const res = await fetch('/api/stakes', { headers: { 'Accept': 'application/json' }});

            if (!res.ok) throw new Error('Failed to load stakes');
            const body = await res.json();
            if (!body.success) throw new Error(body.message || 'Failed');
            const tbody = document.querySelectorAll('table tbody');
            if (!tbody) return console.error('Stakes table body not found');

            tbody.innerHTML = '';

            if (body.stakes.length === 0) {
                const tbody = document.querySelectorAll('table tbody');
                if(tbody.length > 1){
                    let secondTable = tbody[1];
                    if (!secondTable) return console.error('Stakes table body not found');
                    secondTable.innerHTML = '<br><tr><td colspan="6" class="text-center h6">No Recent Stakes</td></tr>';
                }
            } 
            if(tbody.length > 1){
                let secondTable = tbody[1];
                secondTable.innerHTML = '';
            body.stakes.forEach(d => {
                const tr = document.createElement('tr');
                tr.innerHTML = `
                    <td><center>${d.stakeid || d.id}</center></td>
                    <td><center>${d.staketoken || ''}</center></td>
                    <td><center><%=user.currency%> ${Number(d.amount).toFixed(2)}</center></td>
                    <td><center>${d.est_apy || ''}</center></td>
                    <td><center>${d.stakeduration || ''}</center></td>
                    <td><center>${d.stakedate || ''}</center></td>`;
                secondTable.appendChild(tr);
            });
        } } catch (err) {
            console.error('Could not load stakes:', err);
        }
    }
    document.addEventListener('DOMContentLoaded', loadStakes);
</script>

