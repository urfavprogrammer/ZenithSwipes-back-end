<%- include("../partials/header.ejs") %>

<div class="main-content side-content pt-0">
  <div class="main-container container-fluid">
    <div class="inner-body">
      <div id="mobileshow" class="see"></div>
      <div class="sees hide-mobile"></div>
      <div class="mb-5">
        <h4 class="fw-bold text-white">Stake</h4>
        <p class="mb-0">
          Earn profits from staking and manage your staking history
        </p>

        <br /><br />
        <h4 class="fw-bold text-white">Locked Stake</h4>

        <p class="mb-0">
          <i class="fas fa-lock" style="color: #fff"></i>
          <span class="info-box-number">
            <b> &emsp; <%=user.currency %>  <%=asset.total_balance %> </b>
          </span>
        </p>
      </div>

      <div class="container-fluid">
        <div class="table-responsive">
            <% if (error) { %>
            <div class="alert alert-danger"><%= error %></div>
          <% } %>

          <% if (errors && errors.length) { %>
            <div class="alert alert-danger"><ul>
              <% errors.forEach(function(e){ %>
                <li><%= e %></li>
              <% }) %>
            </ul></div>
          <% } %>

          <% if (success) { %>
            <div class="alert alert-success">Your Stake has been placed successfully!</div>
          <% } %>

